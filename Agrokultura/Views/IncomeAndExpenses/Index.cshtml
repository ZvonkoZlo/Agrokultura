@model IEnumerable<Agrokultura.Models.IncomeAndExpense>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AmountOfPlants)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Price)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateOfPlanting)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndDateOfPlanting)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AmountOfGoods)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Plant)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Plot)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AmountOfPlants)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateOfPlanting)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndDateOfPlanting)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AmountOfGoods)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Plant.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Plot.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
